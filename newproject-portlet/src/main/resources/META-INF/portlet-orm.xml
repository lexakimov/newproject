<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.example.sb.model.impl.Entity1ModelImpl">
		<attributes>
			<id name="id">
				<column name="ID_" />
			</id>
			<basic name="Status" />
			<basic name="RequestId" />
			<basic name="Field1" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.example.sb.model.impl.Entity2ModelImpl">
		<attributes>
			<id name="id">
				<column name="ID_" />
			</id>
			<basic name="Status" />
			<basic name="RequestId" />
			<basic name="Field2" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.example.sb.model.impl.Entity1Impl" name="Entity1">
		<table name="SB_Entity1" />
		<attributes />
	</entity>
	<entity class="com.example.sb.model.impl.Entity2Impl" name="Entity2">
		<table name="SB_Entity2" />
		<attributes />
	</entity>
</entity-mappings>