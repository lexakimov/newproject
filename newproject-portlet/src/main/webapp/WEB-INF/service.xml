<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN"
        "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.example.sb">
    <author>akimov</author>
    <namespace>SB</namespace>
    <entity name="Entity1" local-service="false" remote-service="false">
        <column name="id" primary="true" type="long"/>
        <column name="Status" type="String"/>
        <column name="RequestId" type="String"/>
        <column name="Field1" type="String"/>
    </entity>
    <entity name="Entity2" local-service="false" remote-service="false">
        <column name="id" type="long" primary="true"/>
        <column name="Status" type="String"/>
        <column name="RequestId" type="String"/>
        <column name="Field2" type="String"/>
    </entity>
</service-builder>